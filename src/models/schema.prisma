generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id_user   Int       @id @default(cuid())
  name      String
  email     String    @unique
  password  String
  phone     String    @unique
  type_user String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}


